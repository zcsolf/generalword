<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
     <item android:id="@android:id/background">  
  
        <shape>  
  
            <corners android:radius="5dip" />  
  
            <gradient  
                android:angle="0"  
                android:centerColor="#BCBBC1"  
                android:endColor="#BCBBC1"  
                android:startColor="#BCBBC1" />  
        </shape>  
    </item>  
    <item android:id="@android:id/secondaryProgress">  
  
        <clip>  
  
            <shape>  
  
                <corners android:radius="5dip" />  
  
                <gradient  
                    android:angle="0"  
                    android:centerColor="#B62829"  
                  	
                    android:endColor="#B62829"  
                    android:startColor="#B62829" />  
            </shape>  
        </clip>  
    </item>  
  
    <item android:id="@android:id/progress">  
  
        <clip>  
  
            <shape>  
  
                <corners android:radius="5dip" />  
  
                <gradient  
                    android:angle="0"  
                    android:endColor="#B62829"  
                    android:startColor="#B62829" />  
            </shape>  
        </clip>  
    </item>  
    

</layer-list>
